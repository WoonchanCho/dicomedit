{
  "name": "dicomedit",
  "version": "0.0.3",
  "main": "dist/web/dicomedit.min.js",
  "license": "MIT",
  "homepage": "http://github.com",
  "scripts": {
    "clean": "yarn clean:dist && yarn clean:coverage",
    "clean:coverage": "shx rm -rf coverage",
    "clean:dist": "shx rm -rf dist",
    "clean:docs": "shx rm -rf docs",
    "doc:generate": "yarn clean:docs && jsdoc -c jsdoc.conf.js",
    "webpack": "yarn clean:dist && yarn webpack:prod && yarn webpack:dev",
    "webpack:dev": "webpack --progress --config ./config/webpack/webpack-dev.js",
    "webpack:prod": "webpack --progress --config ./config/webpack/webpack-prod.js",
    "webpack:watch": "webpack --progress --watch  --config ./config/webpack",
    "release": "yarn webpack && release-it"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "babel-loader": "^8.2.2",
    "compression-webpack-plugin": "^7.1.1",
    "eslint": "^7.17.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-standard": "^16.0.2",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "esm": "^3.2.25",
    "jsdoc": "^3.6.6",
    "nodemon": "^2.0.6",
    "raw-loader": "^4.0.2",
    "release-it": "^14.2.2",
    "shx": "^0.3.3",
    "terser-webpack-plugin": "^5.0.3",
    "webpack": "^5.11.0",
    "webpack-bundle-analyzer": "^4.3.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@messageformat/core": "^3.0.0-beta.3",
    "dcmjs": "^0.17.0",
    "debug": "^4.3.1",
    "isomorphic-unfetch": "^3.1.0",
    "lodash.clonedeep": "^4.5.0",
    "pegjs": "^0.10.0",
    "pegjs-backtrace": "^0.2.0",
    "uuid": "^8.3.2"
  },
  "sideEffects": false
}
